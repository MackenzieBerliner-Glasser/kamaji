{
  "$schema": "https://raw.githubusercontent.com/Polymer/tachometer/master/config.schema.json",
  "benchmarks": [
    {
      "name": "decimal.js w/ JJ technique",
      "browser": {
        "name": "chrome",
        "headless": false
      },
      "measurement": [
        {
          "mode": "performance",
          "entryName": "perf"
        }
      ],
      "url": "http://localhost:8080/decimalJS_JJ.html"
    },
    {
      "name": "decimal.js w/ BigInt technique",
      "browser": {
        "name": "chrome",
        "headless": false
      },
      "measurement": [
        {
          "mode": "performance",
          "entryName": "perf"
        }
      ],
      "url": "http://localhost:8080/decimalJS_BigInt.html"
    },
    {
      "name": "decimal.js w/ string (control)",
      "browser": {
        "name": "chrome",
        "headless": false
      },
      "measurement": [
        {
          "mode": "performance",
          "entryName": "perf"
        }
      ],
      "url": "http://localhost:8080/control.html"
    }
  ],
  "sampleSize": 50,
  "timeout": 2,
  "resolveBareModules": true,
  "root": "."
}
